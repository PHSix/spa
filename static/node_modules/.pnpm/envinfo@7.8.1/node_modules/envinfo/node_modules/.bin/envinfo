#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ph/Git/spa/static/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/envinfo@7.8.1/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/node_modules:/home/ph/Git/spa/static/node_modules:/home/ph/Git/spa/node_modules:/home/ph/Git/node_modules:/home/ph/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ph/Git/spa/static/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/envinfo@7.8.1/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/node_modules:/home/ph/Git/spa/static/node_modules:/home/ph/Git/spa/node_modules:/home/ph/Git/node_modules:/home/ph/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
