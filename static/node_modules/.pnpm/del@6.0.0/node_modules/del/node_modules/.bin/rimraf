#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ph/Git/spa/static/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/rimraf@3.0.2/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/node_modules:/home/ph/Git/spa/static/node_modules:/home/ph/Git/spa/node_modules:/home/ph/Git/node_modules:/home/ph/node_modules:/home/node_modules:/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/del@6.0.0/node_modules/del/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/del@6.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ph/Git/spa/static/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/rimraf@3.0.2/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/node_modules:/home/ph/Git/spa/static/node_modules:/home/ph/Git/spa/node_modules:/home/ph/Git/node_modules:/home/ph/node_modules:/home/node_modules:/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/del@6.0.0/node_modules/del/node_modules:/home/ph/Git/spa/static/node_modules/.pnpm/del@6.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
fi
